    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "tmp": "^0.2.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit --incremental false --tsBuildInfoFile null && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": "18.15.0",
    "npm": "9.5.0"
  },
  "engineStrict": true
}
